#include <stdio.h>

int main()
{
    int n;
    scanf("%d",&n);
    if(n==1){
        printf("Not a prime number");
        return 0;
    }
    for(int i=2;i<=n/2;i++){
        if(n%i==0){
            printf("It is not a prime number");
            return 0;
        }
    }
    printf("It is a prime number");

    return 0;
}
